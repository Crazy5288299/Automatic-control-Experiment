num1=[1];
den1=[1 1];

num2=[1];
den2=[1 1 1];

num3=[1];
den3=[1 2];

[num4,den4]=parallel(num1,den1,num2,den2);
[numg,deng]=series(num4,den4,num3,den3);

numh=[1];
denh=[1 1];

[num,den]=feedback(numg,deng,numh,denh,-1);
sys=tf(num,den)


%求取系统单位阶跃响应：step()
%求取系统的冲激响应：impulse()
%求取系统任意激励响应：lsim()
t=0:0.01:5;
y_step=step(sys,t);
[y_step,x,t]=step(sys);

y_impulse=impulse(sys,t);
[y_impulse,x,t]=impulse(sys)

u=sin(t)



